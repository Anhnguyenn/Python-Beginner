monthlyInterestRate = annualInterestRate/12.0
lowestPayment = 10
balance2 = balance

for i in range (0,12):
    monthlyUnpaidBalance = balance - lowestPayment
    balance = monthlyUnpaidBalance + (monthlyInterestRate*monthlyUnpaidBalance)

if round(balance) == 0:
    print (lowestPayment)
else:
    while round(balance) > 0:
        balance = balance2
        lowestPayment += 10
        for i in range (0,12):
            monthlyUnpaidBalance = balance - lowestPayment
            balance = monthlyUnpaidBalance + (monthlyInterestRate*monthlyUnpaidBalance)
    print(lowestPayment)
